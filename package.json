{
  "name": "uslunwrapped",
  "version": "0.0.2",
  "private": true,
  "description": "USL soccer league - Match - Player - Team Statistics",
  "license": "UNLICENSED",
  "author": "MiCurran",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "db:dev:rm": "docker compose rm uslunwrapped_dev_db -s -f -v",
    "db:dev:up": "docker compose up uslunwrapped_dev_db -d",
    "db:clean": "npm run clean && npx prisma db push --force-reset && npx prisma db push",
    "db:clean:seed": "npm run db:clean && sleep 1 && npx prisma db seed",
    "db:dev:init": "npm run db:dev:up && sleep 1 && npx prisma migrate dev && npx prisma db seed",
    "clean": "ts-node prisma/cleanDb.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run db:dev:up && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:studio": "npx prisma studio --port 5556",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/apollo": "10.1.7",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^3.1.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "10.1.7",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^5.2.0",
    "@types/cache-manager": "^4.0.3",
    "apollo-server-express": "3.11.1",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "graphql": "16.5.0",
    "graphql-tools": "8.3.14",
    "ioredis": "^5.3.2",
    "jwks-rsa": "^3.0.1",
    "nestjs-pino": "^3.5.0",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.1",
    "@types/cache-manager-redis-store": "^2.0.2",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.12",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "prisma": "^5.2.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "clean": "ts-node prisma/cleanDb.ts"
  }
}
